Sub 筛选()
Dim arr1
Dim a, b
arr1 = Range("")
a = UBound(arr1)

For b = 1 To a
    If arr1(b, 1) = "" Then
        arr1(b, 2) = "Y"
    ElseIf arr1(b, 1) < 5 Then
        arr1(b, 2) = "Y"
    ElseIf arr1(b, 1) = "" Then
        arr1(b, 2) = "Y"
    End If
Next

Range("") = arr1

End Sub

